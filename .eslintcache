ll>\n                  <TableCell align=\"center\">\n                    {recorder.recorder2Name}\n                    <Box>\n                      {recorder.recorder2 ? (\n                        <Button\n                          color=\"primary\"\n                          size=\"small\"\n                          onClick={(e) =>\n                            handleDownload(\n                              DOWNLOAD_RECORDER2_URL,\n                              recorder.id,\n                              recorder.recorder2,\n                              e\n                            )\n                          }\n                        >\n                          {\" \"}\n                          下载\n                        </Button>\n                      ) : (\n                        \"暂未上传\"\n                      )}\n                    </Box>\n                  </TableCell>\n                  <TableCell align=\"center\">\n                    {recorder.summarizerName}\n                    <Box>\n                      {recorder.summary ? (\n                        <Button\n                          color=\"primary\"\n                          size=\"small\"\n                          onClick={(e) =>\n                            handleDownload(\n                              DOWNLOAD_SUMMARY_URL,\n                              recorder.id,\n                              recorder.summary,\n                              e\n                            )\n                          }\n                        >\n                          {\" \"}\n                          下载\n                        </Button>\n                      ) : (\n                        \"暂未上传\"\n                      )}\n                    </Box>\n                  </TableCell>\n                  {hasPermission && (\n                    <TableCell align=\"center\">\n                      <Button\n                        color=\"primary\"\n                        size=\"small\"\n       